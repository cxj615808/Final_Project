<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:id="@+id/linearLayout">

    <TextView
            android:id="@+id/food_name1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="FoodName"
            android:layout_marginTop="21dp"
            app:layout_constraintTop_toBottomOf="@+id/food_date1" android:layout_marginEnd="341dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"/>
    <TextView
            android:id="@+id/food_date1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="FoodDate"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="349dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="21dp"
            app:layout_constraintBottom_toTopOf="@+id/food_name1" app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
            android:id="@+id/F_imageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="19dp" android:layout_marginEnd="25dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="386dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>